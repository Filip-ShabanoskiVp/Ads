@model IEnumerable<Oglasi.Models.Ima>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BrojTransakcijaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KorisnickoImeNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BrojTransakcijaNavigation.BrojTransakcija)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KorisnickoImeNavigation.KorisnickoIme)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.BrojTransakcija">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.BrojTransakcija">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.BrojTransakcija">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
